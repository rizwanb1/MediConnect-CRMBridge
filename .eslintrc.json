{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [],
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "dot-notation": "off",
    "no-unused-expressions": [
      "error",
      {
        "allowTernary": true
      }
    ],
    "no-async-promise-executor": "off"
  }
}